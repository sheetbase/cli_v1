{
  "name": "@sheetbase/cli",
  "version": "1.3.0",
  "description": "Official CLI for working with Sheetbase. ",
  "author": "Sheetbase",
  "homepage": "https://sheetbase.dev",
  "license": "Apache-2.0",
  "main": "./src/index.js",
  "bin": {
    "sheetbase": "./src/index.js"
  },
  "files": [
    "src",
    "!**/*.ts"
  ],
  "preferGlobal": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && npm i -g --loglevel=error",
    "build-fresh": "npm cache clean --force && npm i && npm run build",
    "before-usage": "npm i --save-dev ucfirst extract-comments parse-comments@0.4.3",
    "after-usage": "npm un --save-dev ucfirst extract-comments parse-comments",
    "usage": "npm run before-usage && tsc src/usage.ts --lib es2018,dom && node src/usage.js && npm run after-usage",
    "api": "typedoc ./src --out ./docs/api --mode file --target ES5 --ignoreCompilerErrors --readme none",
    "docs": "npm run usage && npm run api",
    "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
    "test": "nyc --cache false mocha --timeout 100000 tests/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sheetbase/cli.git"
  },
  "bugs": {
    "url": "https://github.com/sheetbase/cli/issues"
  },
  "keywords": [
    "sheetbase",
    "google-apps-script",
    "web",
    "app",
    "api"
  ],
  "importSort": {
    ".ts": {
      "parser": "typescript",
      "style": "module-compact"
    },
    "on-save": false
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "clear": "^0.1.0",
    "commander": "^3.0.0",
    "configstore": "^5.0.0",
    "del": "^5.0.0",
    "figlet": "^1.2.3",
    "fs-extra": "^8.1.0",
    "fs-readdir-recursive": "^1.1.0",
    "google-auth-library": "^5.2.0",
    "inquirer": "^6.5.1",
    "open": "^6.4.0",
    "puppeteer-core": "^1.19.0",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "superstatic": "^6.0.4",
    "tty-table": "^2.7.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/puppeteer-core": "^1.9.0",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
