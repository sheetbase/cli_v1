{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "esnext"
    ],
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": [
    "src/*.ts",
    "tests/*.ts"
  ]
}